{
    "name": "nickolaich/nova-portfolio",
    "description": "Portfolio package for laravel nova",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksii Rytov",
            "email": "lexxukr@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/database": "3.7.*",
        "orchestra/testbench": "3.7.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Nickolaich\\NovaPortfolio\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Nickolaich\\NovaPortfolio\\PortfolioServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
